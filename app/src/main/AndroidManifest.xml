<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.douglassilverio.duckclicker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DuckClicker">
        <activity android:name="br.com.douglassilverio.duckclicker.view.Jogo"></activity>
        <activity android:name="br.com.douglassilverio.duckclicker.view.Dificuldade" />
        <activity android:name="br.com.douglassilverio.duckclicker.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>